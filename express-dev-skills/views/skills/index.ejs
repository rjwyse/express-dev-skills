<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Express Dev Skills</title>
</head>
<body>
    <a href="/">home</a>
    <h1>All Dev-Skills</h1>
    <ul>
      <% skills.forEach(function(t) { %>
        <li>
          <a href="/skills/<%= t.id %>"><%= t.skill %></a>
            - 
          <%= t.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>